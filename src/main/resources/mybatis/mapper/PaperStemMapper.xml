<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.czxy.manage.dao.PaperStemMapper">

    <insert id="insert" useGeneratedKeys="true" keyProperty="id">
        insert into w_paper_stem  (`index`, paper_id, stem_id) VALUES (#{index},#{paperId},#{id})
    </insert>
    <delete id="delete">
        delete from w_paper_stem where paper_id=#{id}
    </delete>
</mapper>