<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.czxy.manage.dao.StemMapper">
    <insert id="insert" useGeneratedKeys="true" keyProperty="id">
        insert into w_stem
            (title, score, `index`, required)
        values (#{title}, #{score}, #{required})
    </insert>
    <update id="update">
        update w_stem
        set title        = #{title},
            score        = #{score},
            required     = #{required},
            updated_time = now()
    </update>
    <delete id="delete">
        delete from w_option where id in
        <foreach collection="list" item="item" open="(" close=")" separator=",">
            #{item}
        </foreach>
    </delete>
    <select id="query" resultType="com.czxy.manage.model.entity.questionnaire.stem.StemEntity">

    </select>
</mapper>